<?xml version="1.0" encoding="utf-8"?>
<!--FIXME: Stub to create different result screens. Remove all-->
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".gui.ChooseResults">

    <Button
        android:id="@+id/stubResultWin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Win"
        app:layout_constraintBottom_toTopOf="@+id/stubResultLoose"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/stubResultLoose"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Loose"
        app:layout_constraintBottom_toTopOf="@+id/stubResultDraw"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/stubResultWin" />

    <Button
        android:id="@+id/stubResultDraw"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Draw"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/stubResultLoose" />
</androidx.constraintlayout.widget.ConstraintLayout>